						"left": "calc(var(--wp--custom--spacing--base) * 2)",
						"right": "0"
					},
					"margin": {
						"top": "calc(var(--wp--custom--spacing--base) * 2) !important",
						"bottom": "calc(var(--wp--custom--spacing--base) * 2) !important"
					}
				},
				"elements": {
					"cite": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal"
						}
					}
				}
			},
			"core/pullquote": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"lineHeight": "1.4108"
				},
				"border": {
					"style": "solid",
					"width": "1px 0",
					"color": "var(--wp--preset--color--contrast-border)"
				},
				"spacing": {
					"margin": {
						"bottom": "calc(var(--wp--custom--spacing--base) * 2) !important",
						"top": "calc(var(--wp--custom--spacing--base) * 2) !important"
					},
					"padding": {
						"bottom": "var(--wp--custom--spacing--base)",
						"top": "var(--wp--custom--spacing--base)"
					}
				},
				"elements": {
					"cite": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						},
						"typography": {
							"fontSize": "var(--wp--preset--font-size--small)",
							"fontStyle": "normal",
							"textTransform": "none"
						}
					}
				}
			},
			"core/query": {
				"elements": {
					"h2": {
						"typography": {
							"fontSize": "var(--wp--preset--font-size--x-large)",
