					"slug": "xxxx-large"
				},
				{
					"fluid": false,
					"size": "clamp(2.99rem, 8.02vw + -0.02rem, 7rem)",
					"slug": "xxxxx-large"
				},
				{
					"fluid": false,
					"size": "clamp(3.5rem, 12vw + -1rem, 9.5rem)",
					"slug": "xxxxxx-large"
