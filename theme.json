{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 2,
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"custom": {
			"color": {
				"base": "255, 255, 255",
				"contrast": "0, 0, 0"
			},
			"fontWeight": {
				"text": "400",
				"heading": "400"
			},
			"lineHeight": "1.625",
			"letterSpacing": {
				"text": "-0.015em",
				"heading": "-0.025em"
			},
			"spacing": {
				"base": "calc(var(--wp--preset--font-size--normal) * var(--wp--custom--line-height))"
			}
		},
		"color": {
			"palette": [
				{
					"color": "rgb(var(--wp--custom--color--base))",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "rgba(var(--wp--custom--color--base), 0.87)",
					"name": "Base Text",
					"slug": "base-text"
				},
				{
					"color": "rgba(var(--wp--custom--color--base), 0.4)",
					"name": "Base Secondary Text",
					"slug": "base-secondary-text"
				},
				{
					"color": "rgba(var(--wp--custom--color--base), 0.21)",
					"name": "Base Text Underline",
					"slug": "base-text-underline"
				},
				{
					"color": "rgba(var(--wp--custom--color--base), 0.12)",
					"name": "Base Border",
					"slug": "base-border"
				},
				{
					"color": "rgba(var(--wp--custom--color--base), 0.03)",
					"name": "Base Secondary Background",
					"slug": "base-secondary-bg"
				},
				{
					"color": "rgb(var(--wp--custom--color--contrast))",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgba(var(--wp--custom--color--contrast), 0.87)",
					"name": "Contrast Text",
					"slug": "contrast-text"
				},
				{
					"color": "rgba(var(--wp--custom--color--contrast), 0.4)",
					"name": "Contrast Secondary Text",
					"slug": "contrast-secondary-text"
				},
				{
					"color": "rgba(var(--wp--custom--color--contrast), 0.21)",
					"name": "Contrast Text Underline",
					"slug": "contrast-text-underline"
				},
				{
					"color": "rgba(var(--wp--custom--color--contrast), 0.12)",
					"name": "Contrast Border",
					"slug": "contrast-border"
				},
				{
					"color": "rgba(var(--wp--custom--color--contrast), 0.03)",
					"name": "Contrast Secondary Background",
					"slug": "contrast-secondary-bg"
				},
				{
					"color": "#e3051a",
					"name": "Primary",
					"slug": "primary"
				}
			]
		},
		"layout": {
			"contentSize": "max(640px, 40vw)",
			"wideSize": "max(1280px, 80vw)"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "calc(var(--wp--custom--spacing--base) * 0.5)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "var(--wp--custom--spacing--base)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(1.625rem, 2.2344vw + 0.0889rem, 2.7422rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(2.4375rem, 2.4375vw + 0.7617rem, 3.6563rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(2.375rem, 6.2188vw + -1.9004rem, 5.4844rem)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "clamp(4.875rem, 8.5313vw + -0.9902rem, 9.1406rem)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "clamp(6.5rem, 16.25vw + -4.6719rem, 14.625rem)",
					"slug": "70",
					"name": "7"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "\"Helvetica Neue\",Arial,-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "ui-monospace, SFMono-Regular, SF Mono, Menlo, Consolas, Liberation Mono, monospace",
					"name": "Monospace Font",
					"slug": "monospace-font"
				}
			],
			"fontSizes": [
				{
					"fluid": false,
					"size": "calc(var(--wp--preset--font-size--normal) * 0.875)",
					"slug": "small"
				},
				{
					"fluid": false,
					"size": "clamp(1rem, 0.25vw + 0.8281rem, 1.125rem)",
					"slug": "normal"
				},
				{
					"fluid": false,
					"size": "clamp(1.125rem, 0.45vw + 0.8156rem, 1.35rem)",
					"slug": "medium"
				},
				{
					"fluid": false,
					"size": "clamp(1.2656rem, 0.7087vw + 0.7784rem, 1.62rem)",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "clamp(1.4238rem, 1.0403vw + 0.7086rem, 1.944rem)",
					"slug": "x-large"
				},
				{
					"fluid": false,
					"size": "clamp(1.6018rem, 1.462vw + 0.5967rem, 2.3328rem)",
					"slug": "xx-large"
				},
				{
					"fluid": false,
					"size": "clamp(1.802rem, 1.9947vw + 0.4307rem, 2.7994rem)",
					"slug": "xxx-large"
				},
				{
					"fluid": false,
					"size": "clamp(2.0273rem, 2.6639vw + 0.1959rem, 3.3592rem)",
					"slug": "xxxx-large"
				},
				{
					"fluid": false,
					"size": "clamp(3rem, 5.0667vw + 1.7333rem, 7.75rem)",
					"slug": "xxxxx-large"
				},
				{
					"fluid": false,
					"size": "clamp(3.5rem, 8.5333vw + 1.3667rem, 11.5rem)",
					"slug": "xxxxxx-large"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "calc(var(--wp--custom--spacing--base) * 2)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							},
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/navigation": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "calc(var(--wp--custom--spacing--base) * 2)"
				},
				"spacing": {
					"blockGap": "0 calc(var(--wp--custom--spacing--base) * 1.375)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text) !important"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text) !important"
							},
							"typography": {
								"textDecoration": "none"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text) !important"
							},
							"typography": {
								"textDecoration": "none"
							}
						}
					}
				}
			},
			"core/post-title": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--xxxxx-large)",
					"lineHeight": "1"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-content": {
				"color": {
					"text": "var(--wp--preset--color--contrast-text)"
				},
				"typography": {
					"lineHeight": "var(--wp--custom--line-height)"
				}
			},
			"core/post-author": {
				"color": {
					"text": "var(--wp--preset--color--contrast-secondary-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-excerpt": {
				"color": {
					"text": "var(--wp--preset--color--contrast-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)"
				}
			},
			"core/post-date": {
				"color": {
					"text": "var(--wp--preset--color--contrast-secondary-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						}
					}
				}
			},
			"core/post-terms": {
				"color": {
					"text": "var(--wp--preset--color--contrast-secondary-text)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						}
					}
				}
			},
			"core/comments-title":{
				"spacing": {
					"margin": {
						"top": "0",
						"bottom": "var(--wp--preset--spacing--30)"
					}
				}
			},
			"core/comment-author-name": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "500"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							}
						}
					}
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast-secondary-text)"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast-secondary-text)"
							}
						}
					}
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--spacing--base)"
				},
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--contrast)"
						},
						"typography": {
							"textDecoration": "none"
						},
						"border": {
							"width": "1px",
							"style": "solid",
							"color": "var(--wp--preset--color--contrast-text-underline)",
							"radius": "3rem"
						},
						"spacing": {
							"padding": {
								"top": "0.25em",
								"right": "1em",
								"bottom": "0.25em",
								"left": "1em"
							}
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--base)",
								"background": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "none"
							},
							"border": {
								"color": "var(--wp--preset--color--primary)"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--base)",
								"background": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "none"
							},
							"border": {
								"color": "var(--wp--preset--color--primary)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--base)",
								"background": "var(--wp--preset--color--primary)"
							},
							"typography": {
								"textDecoration": "none"
							},
							"border": {
								"color": "var(--wp--preset--color--primary)"
							}
						}
					}
				},
				"css": "& a {\n\tdisplay: inline-block;\n\t}"
			},
			"core/comment-content": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--normal)",
					"lineHeight": "var(--wp--custom--line-height)"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast-text)"
				}
			},
			"core/comments-pagination": {
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--30) !important",
						"bottom": "var(--wp--preset--spacing--60)"
					}
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/quote": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "1.3542"
				},
				"border": {
					"width": "0"
				},
				"spacing": {
