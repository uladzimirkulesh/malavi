				},
				{
					"fluid": false,
					"size": "clamp(1.27rem, 0.35vw + 1.13rem, 1.44rem)",
					"slug": "large"
				},
				{
					"fluid": false,
					"size": "clamp(1.42rem, 0.61vw + 1.2rem, 1.73rem)",
