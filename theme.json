		"layout": {
			"contentSize": "max(640px, 40vw)",
			"wideSize": "max(1280px, 80vw)"
		},
		"spacing": {
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "calc(var(--wp--custom--spacing--base) * 0.5)",
					"slug": "10",
					"name": "1"
				},
				{
					"size": "var(--wp--custom--spacing--base)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(1.63rem, 1.63vw + 1.02rem, 2.44rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(2.44rem, 1.63vw + 1.83rem, 3.25rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(2.44rem, 3.25vw + 1.22rem, 4.06rem)",
